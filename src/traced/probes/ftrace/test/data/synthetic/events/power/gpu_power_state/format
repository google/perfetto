name: gpu_power_state
ID: 1888
format:
	field:unsigned short common_type;	offset:0;	size:2;	signed:0;
	field:unsigned char common_flags;	offset:2;	size:1;	signed:0;
	field:unsigned char common_preempt_count;	offset:3;	size:1;	signed:0;
	field:int common_pid;	offset:4;	size:4;	signed:1;

	field:int old_state;	offset:8;	size:4;	signed:1;
	field:int new_state;	offset:12;	size:4;	signed:1;

print fmt: "power state changing from %s to %s", __print_symbolic(REC->old_state, {PIXEL_GPU_POWER_STATE_OFF, "OFF"}, {PIXEL_GPU_POWER_STATE_PG, "PG"}, {PIXEL_GPU_POWER_STATE_ON, "ON"}), __print_symbolic(REC->new_state, {PIXEL_GPU_POWER_STATE_OFF, "OFF"}, {PIXEL_GPU_POWER_STATE_PG, "PG"}, {PIXEL_GPU_POWER_STATE_ON, "ON"})
